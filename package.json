{
    "name": "country-data-list",
    "version": "1.3.4",
    "description": "Data about countries - like their ISO codes and currencies",
    "homepage": "https://github.com/Sonatrix/country-list",
    "bugs": {
        "url": "https://github.com/Sonatrix/country-list/issues"
    },
    "main": "dist/index.js",
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "prettier --config .prettierrc --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "scripts": {
        "build": "rm -r dist/* && npx tsc && babel src -d dist",
        "test": "mocha",
        "lint": "eslint src",
        "prepare": "npm run lint --fix && npm run test && npm run build",
        "prettify": "prettier --config .prettierrc --write '*/**/*.js'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Sonatrix/country-list.git"
    },
    "keywords": [
        "data",
        "country",
        "es6",
        "es2015",
        "babel",
        "mocha",
        "chai",
        "eslint",
        "travis",
        "travis-ci"
    ],
    "author": "Sonatrix",
    "license": "MIT",
    "dependencies": {
        "currency-symbol-map": "~5"
    },
    "devDependencies": {
        "@babel/cli": "^7.24.8",
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.25.3",
        "@babel/register": "^7.24.6",
        "babel-eslint": "10.1.0",
        "chai": "4.3.7",
        "eslint": "^9.9.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^9.1.4",
        "lint-staged": "^15.2.9",
        "lodash": ">=4.17.21",
        "mixin-deep": ">=2.0.1",
        "mocha": "^10.2.0",
        "prettier": "3.3.3",
        "set-value": ">=4.1.0",
        "typescript": "^5.5.4"
    }
}
